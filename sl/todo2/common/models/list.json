  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "Item",
      "foreignKey": ""
    },
    "person": {
      "type": "belongsTo",
